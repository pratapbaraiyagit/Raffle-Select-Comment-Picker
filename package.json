  "dependencies": {
    "@fortawesome/react-fontawesome": "0.2.0",
    "@reduxjs/toolkit": "1.9.5",
    "antd": "5.8.5",
    "axios": "1.4.0",
    "bootstrap": "^5.2.3",
    "copy-to-clipboard": "3.3.3",
    "cra-template-rb": "1.2.6",
    "crypto-js": "4.1.1",
    "formik": "2.4.3",
    "html-to-image": "1.11.11",
